{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ATTRIBUTES": [
            {
              "GLOBAL": "ANONYMOUS"
            }
          ],
          "RIGHTS": [
            "READ"
          ],
          "ACCESS": "ALLOW"
        },
        "OBJECTS": [
          {
            "ROUTE": "*"
          }
        ],
        "FORMULA": {
          "$and": [
            {
              "$eq": [
                {
                  "REFERENCE": {
                    "$sme("SubmodelID-OperationalData").machineState#value"
                  }
                },
                {
                  "$strVal": "not-running"
                }
              ]
            },
            {
              "$or": [
                {
                  "$eq": [
                    {
                      "$field": "$sm#semanticId"
                    },
                    {
                      "$strVal": "SemanticID-Nameplate"
                    }
                  ]
                },
                {
                  "$eq": [
                    {
                      "$field": "$sm#semanticId"
                    },
                    {
                      "$strVal": "SemanticID-TechnicalData"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ]
  }
}
