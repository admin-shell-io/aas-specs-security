{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ATTRIBUTES": [
            {
              "CLAIM": "companyName"
            }
          ],
          "RIGHTS": [
            "READ"
          ],
          "ACCESS": "ALLOW"
        },
        "OBJECTS": [
          {
            "IDENTIFIABLE": "$sm(\"*\")"
          }
        ],
        "FORMULA": {
          "$and": [
            {
              "$or": [
                {
                  "$eq": [
                    {
                      "$field": "$sm#semanticId"
                    },
                    {
                      "$strVal": "SemanticID-Nameplate"
                    }
                  ]
                },
                {
                  "$eq": [
                    {
                      "$field": "$sm#semanticId"
                    },
                    {
                      "$strVal": "SemanticID-TechnicalData"
                    }
                  ]
                }
              ]
            },
            {
              "$eq": [
                {
                  "$attribute": {
                    "CLAIM": "companyName"
                  }
                },
                {
                  "$strVal": "company1-name"
                }
              ]
            },
            {
              "$regex": [
                {
                  "$attribute": {
                    "REFERENCE": "$sm#id"
                  }
                },
                {
                  "$strVal": "^https://company1.com/.*$"
                }
              ]
            },
            {
              "$ge": [
                {
                  "$timeCast": {
                    "$attribute": {
                      "GLOBAL": "UTCNOW"
                    }
                  }  
                },
                {
                  "$timeVal": "09:00"
                }
              ]
            },
            {
              "$le": [
                {
                  "$timeCast": {
                    "$attribute": {
                      "GLOBAL": "UTCNOW"
                    }
                  }  
                },
                {
                  "$timeVal": "17:00"
                }
              ]
            }
          ]
        }
      }
    ]
  }
}
