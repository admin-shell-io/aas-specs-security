{
  "AllAccessPermissionRules": {
      "ATTRIBUTES": [
        {
          "CLAIM": "companyName"
        }
      ],
      "RIGHTS": [
        "READ"
      ],
      "ACCESS": "ALLOW",
      "OBJECTS": [
        {
          "IDENTIFIABLE": "(Submodel)*"
        }
      ],
      "FORMULA": {
        "$and": [
          {
            "$or": [
              {
                "$eq": [
                  {
                    "$field": "$sm#semanticId"
                  },
                  {
                    "$strVal": "SemanticID-Nameplate"
                  }
                ]
              },
              {
                "$eq": [
                  {
                    "$field": "$sm#semanticId"
                  },
                  {
                    "$strVal": "SemanticID-TechnicalData"
                  }
                ]
              }
            ]
          },
          {
            "$eq": [
              {
                "$attribute": {
                  "CLAIM": "companyName"
                }
              },
              {
                "$strVal": "company1-name"
              }
            ]
          },
          {
            "$regex": [
              {
                "$attribute": {
                  "REFERENCE": "(Submodel)*#Id"
                }
              },
              {
                "$strVal": "^https://company1.com/.*$"
              }
            ]
          },
          {
            "$ge": [
              {
                "$attribute": {
                  "GLOBAL": "UTCNOW"
                }
              },
              {
                "$timeVal": "09:00"
              }
            ]
          },
          {
            "$le": [
              {
                "$attribute": {
                  "GLOBAL": "UTCNOW"
                }
              },
              {
                "$timeVal": "17:00"
              }
            ]
          }
        ]
      }
    }
}
